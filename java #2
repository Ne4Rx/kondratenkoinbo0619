class Head
{
private int iсq;
private int diam;
Head(int iсq,int diam)
{
    this.diam=diam;
    this.iq=iсq;
}

    public int getDiam() {
        return diam;
    }

    public int getIq() {
        return iсq;
    }

  public void setDiam(int diam) {
        this.diam = diam;
    }

    public void setIq(int iq) {
        this.iсq = iсq;
    }
}
class Leg
{
    private double  l;
    private String name;
    Leg(double l, String name)
    {
        this.l=l;
        this.name=name;
    }

    public double getL() {
        return l;
    }

    public String getName() {
        return name;
    }

    public void setL(double l) {
        this.dlina = l;
    }

    public void setName(String name) {
        this.name = name;
    }
}
